<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoFinal_Xamarin.RegisterPage">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Entry x:Name="entryName" Placeholder="Your Name"></Entry>
            <Entry x:Name="entryLastName" Placeholder="Your Last Name"></Entry>
            <Entry x:Name="entryEmail" Placeholder="Email"></Entry>
            <Entry x:Name="entryPassword"></Entry>
            <Entry x:Name="entryConfirmPassword"></Entry>
            <Button Text="Register" Clicked="Register"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>